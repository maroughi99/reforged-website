{
  "name": "warcraft3-reforged-website",
  "version": "1.0.0",
  "description": "Warcraft 3 Reforged Ladder & Community Website",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --ignore '*.json' --ignore '*.log' --ignore '*.txt' server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "fetch-content": "node auto-fetch-all-sources.js --once",
    "auto-fetch": "node auto-fetch-all-sources.js",
    "fetch-maps": "node fetch-maps.js"
  },
  "keywords": [
    "warcraft3",
    "reforged",
    "ladder",
    "gaming"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "multer": "^2.0.2",
    "node-schedule": "^2.1.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
